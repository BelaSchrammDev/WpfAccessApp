<Window x:Class="WpfApp1.Views.Users"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.Views"
        mc:Ignorable="d"
        Title="Users" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGridUsers" Margin="10,10,10,0" VerticalAlignment="Top" Height="235" Grid.ColumnSpan="2"/>
        <Button Content="Show Users" Margin="10,10,10,10" VerticalAlignment="Bottom" Click="ListUsers" Height="20" Grid.Column="1" HorizontalAlignment="Right" Width="90"/>
    </Grid>
</Window>
