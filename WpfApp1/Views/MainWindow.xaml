<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="171*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="dataGridUsers" Margin="10,10,10,0" VerticalAlignment="Top" Height="235" Grid.ColumnSpan="2"/>
        <Button Content="Userwindow" Margin="10,10,10,10" VerticalAlignment="Bottom" Click="OpenUserWindow" Height="20" Grid.Column="1" HorizontalAlignment="Right" Width="90"/>
        <Button Content="Insert Vehicles" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="90" Click="InsertVehicle" Height="20" Margin="0,0,19,10"/>
        <Button Content="Show Vehicles" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="90" Click="ListVehicles" Height="20" Margin="0,0,119,10"/>
    </Grid>
</Window>
